{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true
  },
  "paths": {
    "@mui/styled-engine": ["./node_modules/@mui/styled-engine-sc"]
  },
  "include": ["src", "**/*.ts", "**/*.tsx"]
}
